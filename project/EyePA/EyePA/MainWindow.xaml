<Window x:Class="EyePA.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="851.4" Width="1701" SizeChanged="Window_SizeChanged">
    <Grid>
        <ListView x:Name="GUIListView" HorizontalAlignment="Left" Height="200" Margin="64,567,0,0" VerticalAlignment="Top" Width="1589" SelectionChanged="GUIListView_SelectionChanged">
            <ListView.ItemsPanel>
                <ItemsPanelTemplate>
                    <StackPanel Orientation="Horizontal" ></StackPanel>
                </ItemsPanelTemplate>
            </ListView.ItemsPanel>
        </ListView>
        <Label x:Name="GUIFolderPath" Content="chemin" HorizontalAlignment="Left" Margin="64,784,0,0" VerticalAlignment="Top" Width="88"/>
        <Button x:Name="GUIBtnQuit" Content="Quit" HorizontalAlignment="Left" Height="133" Margin="1443,25,0,0" VerticalAlignment="Top" Width="210" Click="GUIBtnQuit_Click"/>
        <Button x:Name="GUIBtnBrowse" Content="Change Folder" HorizontalAlignment="Left" Height="118" Margin="1443,187,0,0" VerticalAlignment="Top" Width="210" Click="GUIBtnBrowse_Click"/>
        <Canvas x:Name="GUIBigPicture" HorizontalAlignment="Left" Height="490" Margin="64,47,0,0" VerticalAlignment="Top" Width="922"/>
        <Image x:Name="GUIImageTest" HorizontalAlignment="Left" Height="133" Margin="952,36,0,0" VerticalAlignment="Top" Width="143"/>

    </Grid>
</Window>
