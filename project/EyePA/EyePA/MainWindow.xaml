<Window x:Class="EyePA.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="655" Width="1220">
    <Grid>
        <ListView x:Name="GUIListView" HorizontalAlignment="Left" Height="200" Margin="64,383,0,0" VerticalAlignment="Top" Width="1031">
            <ListView.ItemsPanel>
                <ItemsPanelTemplate>
                    <StackPanel Orientation="Horizontal"></StackPanel>
                </ItemsPanelTemplate>
            </ListView.ItemsPanel>
        </ListView>
        <Label x:Name="GUIFolderPath" Content="chemin" HorizontalAlignment="Left" Margin="64,588,0,0" VerticalAlignment="Top" Width="88"/>
        <Button x:Name="GUIBtnQuit" Content="Quit" HorizontalAlignment="Left" Height="133" Margin="885,36,0,0" VerticalAlignment="Top" Width="210"/>
        <Button x:Name="GUIBtnBrowse" Content="Change Folder" HorizontalAlignment="Left" Height="118" Margin="885,187,0,0" VerticalAlignment="Top" Width="210" Click="GUIBtnBrowse_Click"/>
        <Canvas x:Name="GUIBigPicture" HorizontalAlignment="Left" Height="316" Margin="64,47,0,0" VerticalAlignment="Top" Width="614"/>
        <Image x:Name="GUIImageTest" HorizontalAlignment="Left" Height="133" Margin="952,36,0,0" VerticalAlignment="Top" Width="143"/>

    </Grid>
</Window>
